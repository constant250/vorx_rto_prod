{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "barryvdh/laravel-dompdf": "^0.8.5",
        "bluedogtraining/avetmiss": "^4.5",
        "cartalyst/stripe-laravel": "^11.0",
        "daltcore/lara-pdf-merger": "^2.0",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.0",
        "intervention/image": "^2.5",
        "ixudra/curl": "^6.21",
        "laracasts/utilities": "^3.0",
        "laravel/framework": "^6.2",
        "laravel/tinker": "^1.0",
        "laravel/ui": "^1.1",
        "laravelista/lumen-vendor-publish": "^6.0",
        "league/omnipay": "^3.0",
        "messagebird/php-rest-api": "^1.17",
        "mpdf/mpdf": "^8.0",
        "owen-it/laravel-auditing": "^9.3",
        "phpmailer/phpmailer": "^6.1",
        "phpoffice/phpspreadsheet": "^1.13",
        "rambler-digital-solutions/php-json-rpc": "@stable",
        "rguedes/pdfmerger": "^1.0",
        "simplesoftwareio/simple-qrcode": "~2",
        "spatie/laravel-cors": "^1.6",
        "spatie/laravel-permission": "^3.13",
        "stripe/stripe-php": "^7.67",
        "sudiptpa/omnipay-nabtransact": "^3.0",
        "tightenco/ziggy": "^0.8.1"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
